{
  "mcpServers": {
    "api-supermemory-ai": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://api.supermemory.ai/mcp"],
      "env": {},
      "headers": {
        "Authorization": "${SUPERMEMORY_API_KEY}"
      }
    },
    "browser-use": {
      "command": "uvx",
      "args": ["browser-use[cli]==0.5.10", "--mcp"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "repomix": {
      "command": "npx",
      "args": ["-y", "repomix", "--mcp"]
    },
    "zen": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/BeehiveInnovations/zen-mcp-server.git",
        "zen-mcp-server"
      ],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "GEMINI_API_KEY": "${GEMINI_API_KEY}",
        "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}"
      }
    }
  }
}
